    "md5": "^2.3.0",
    "sass": "^1.69.5",
    "scss": "^0.2.4",
    "unplugin-element-plus": "^0.8.0",
    "vue": "^3.3.4",
    "vue-cookies": "^1.8.3",
    "vue-router": "^4.2.5",
    "vue-session": "^1.0.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
